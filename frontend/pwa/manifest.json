{
    "manifest_version": 3,
    "name": "u2.os",
    "short_name": "u2.os",
    "version": "canary",
    "start_url": "/frontend/",
    "author": "u2re-dev",
    "id": "/",
    "scope": "/",
    "display": "fullscreen",
    "display_override": [
        "window-controls-overlay",
        "borderless",
        "fullscreen",
        "standalone"
    ],
    "description": "Web OS-like",
    "theme_color": "#000000",
    "background_color": "#000000",
    "orientation": "any",
    "categories": [
        "personalization",
        "education",
        "entertainment",
        "lifestyle"
    ],
    "permissions": ["window-management", "fullscreen", "theme"],
    "icons": [{
        "src": "../assets/icons/home/512x.png",
        "sizes": "512x512",
        "type": "image/png",
        "purpose": "any"
    }, {
        "src": "../assets/icons/home/512x.png",
        "sizes": "512x512",
        "type": "image/png",
        "purpose": "maskable"
    }],
    "incognito": "split",
    "default_locale": "en"
}
