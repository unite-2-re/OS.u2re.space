{
    "manifest_version": 3,
    "name": "os.u2re.space",
    "short_name": "os.u2re.space",
    "version": "canary",
    "start_url": "/frontend/",
    "author": ["u2re-dev"],
    "id": "/",
    "scope": "/",
    "display": "fullscreen",
    "display_override": [
        "window-controls-overlay",
        "borderless",
        "fullscreen",
        "standalone"
    ],
    "description": "U2RE Workspace App",
    "theme_color": "#101010",
    "background_color": "#101010",
    "orientation": "any",
    "categories": [
        "personalization",
        "education",
        "entertainment",
        "lifestyle"
    ],
    "permissions": ["window-management", "fullscreen", "theme"],
    "icons": [{
        "src": "../assets/icons/home/512x.png",
        "sizes": "512x512",
        "type": "image/png",
        "purpose": "any"
    }, {
        "src": "../assets/icons/home/512x.png",
        "sizes": "512x512",
        "type": "image/png",
        "purpose": "maskable"
    }],
    "incognito": "split",
    "default_locale": "en"
}
